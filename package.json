{
  "name": "@theclinician/ddp-connector",
  "version": "0.0.10",
  "description": "DDP client bindings for react-redux",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --ignore '*.test.js'",
    "build-webpack": "webpack --progress",
    "clean": "rimraf lib coverage",
    "coverage": "babel-node $(npm bin)/isparta cover _mocha -- 'src/**/*.test.js'",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm run build",
    "test": "mocha --compilers js:babel-core/register 'src/**/*.test.js'",
    "test-watch": "mocha --watch --compilers js:babel-core/register 'src/**/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theclinician/ddp-connector.git"
  },
  "keywords": [],
  "author": "Tomasz Lenarcik <apendua@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theclinician/ddp-connector/issues"
  },
  "homepage": "https://github.com/theclinician/ddp-connector",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-root-import": "^4.1.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "clean-webpack-plugin": "^0.1.15",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.2.0",
    "isparta": "^4.0.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "@theclinician/ddp-client": "0.0.5",
    "@theclinician/toolbelt": "0.0.2",
    "ejson": "^2.1.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
